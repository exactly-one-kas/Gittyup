  <files>
    <file src="tools\**" target="tools" />
    <file src="@GITTYUP_NAME@-win32-@GITTYUP_VERSION@@BUILD_TAG@.exe" target="tools\setup-32.exe" />
    <file src="@GITTYUP_NAME@-win64-@GITTYUP_VERSION@@BUILD_TAG@.exe" target="tools\setup-64.exe" />
  </files>
</package>
