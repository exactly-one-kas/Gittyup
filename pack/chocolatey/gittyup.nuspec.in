<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>gittyup</id>
    <version>@GITTYUP_VERSION@</version>
    <packageSourceUrl>https://github.com/Murmele/Gittyup/tree/master/pack/chocolatey</packageSourceUrl>
    <owners>exactly_one_kas</owners>
    <title>Gittyup</title>
    <authors>https://github.com/gitahead/gitahead/graphs/contributors</authors>
    <projectUrl>https://github.com/Murmele/Gittyup</projectUrl>
    <iconUrl>https://cdn.jsdelivr.net/gh/Murmele/Gittyup@@GITTYUP_BUILD_REVISION@/rsrc/Gittyup.iconset/icon_128x128.png</iconUrl>
    <copyright>Copyright (c) 2018 Scientific Toolworks, Inc. Copyright (c) 2021 Gittyup contributors</copyright>
    <licenseUrl>https://github.com/Murmele/Gittyup/blob/master/LICENSE.md</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectSourceUrl>https://github.com/Murmele/Gittyup</projectSourceUrl>
    <docsUrl>https://stackoverflow.com/questions/tagged/gittyup</docsUrl>
    <bugTrackerUrl>https://github.com/Murmele/Gittyup/issues</bugTrackerUrl>
    <tags>gittyup git client gui foss</tags>
    <summary>GitAhead is a graphical Git client designed to help you understand and manage your source code history</summary>
    <description>
      Gittyup is a graphical Git client designed to help you understand and manage your source code history.

      Gittyup is a continuation of the GitAhead client.
    </description>
